{
    "name": "cypress-allure-plugin-example",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "cy:open": "yarn cypress open --env allure=true --config integrationFolder=cypress/integration/examples,testFiles=**/*.js --browser chrome",
        "cy:run": "yarn cypress run --config integrationFolder=cypress/integration/examples --env allure=true --browser chrome",
        "allure:report": "allure generate allure-results --clean -o allure-report",
        "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true"
    },
    "devDependencies": {
        "@shelex/cypress-allure-plugin": "^1.1.0",
        "cypress": "^4.1.0"
    }
}
